    "saveEif": "/_api/Lists/getByTitle('{LIST_NAME}')/items",
    "delete": "/_api/Lists/getByTitle('{LIST_NAME}')/items('{ITEM_ID}')",
    "update": "/_api/Lists/getByTitle('{LIST_NAME}')/items('{ITEM_ID}')",
    "getEifDraftsForUser": "/_api/Lists/getByTitle('{LIST_NAME}')/items?$filter=TAR_Requester eq {USER_ID}&$select=Id,Title,TAR_Requester/Title,TAR_Destinations,TAR_FirstName,TAR_LastName,TAR_TarId&$expand=TAR_Requester",
    "getEifSubmittedForUser": "/_api/Lists/getByTitle('{LIST_NAME}')/items?$filter=TAR_Requester eq {USER_ID}&$select=Id,Title,TAR_Requester/Title,TAR_Destinations,TAR_FirstName,TAR_LastName,TAR_TarId,{EXPAND_USERS},TAR_Requester"
